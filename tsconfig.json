{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports" : true,
    "downlevelIteration" : true,
    "composite": true,
    "skipLibCheck": true,   
    "resolveJsonModule": true,
    "baseUrl": ".",
    "esModuleInterop": true
  }
}
